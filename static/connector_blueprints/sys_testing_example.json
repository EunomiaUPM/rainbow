      "urlTemplate": "http://cb.com:1254/subscribe/{{__SYS_BLA__}}",
      "method": "DELETE",
      "headers": "{{__HEADERS__}}"
    }
  },
  "parameters": [
    {
      "paramType": "STRING",
      "name": "ATTR",
      "title": "bla",
      "required": true
    },
    {
      "paramType": "MAP<STRING,STRING>",
      "name": "HEADERS",
      "title": "bla",
      "required": true
    },
    {
      "paramType": "STRING",
      "name": "SYS_BLA",
      "title": "sys bla",
      "required": true,
      "autoFilled": true,
      "autoFilledType": "SYS_URN"
    }
  ]
